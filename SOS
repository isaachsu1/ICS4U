#Define the Function
def SOS():
  #Promt the user for a number n
  n = int(input("Enter the value of n: "))
  #Initialize a variable total to 0
  total = 0
  #Use a loop to iterate i from 1 to n
  for i in range(1, n+1):
    #Add i*i to total
      total += i*i
  return total
  #Print total
  print(total)


SOS()
